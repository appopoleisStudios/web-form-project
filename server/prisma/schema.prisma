generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id                   Int      @id @default(autoincrement())
  fullName             String
  email                String   @unique
  spokenLanguages      String[]
  codingLanguages      Json
  yearsExperience      Int
  blockchainExperience String
  createdAt            DateTime @default(now())
}
